{
  "kind": "collectionType",
  "collectionName": "messages",
  "info": {
    "name": "Message"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 20,
      "minLength": 2
    },
    "surname": {
      "type": "string",
      "maxLength": 20,
      "minLength": 2
    },
    "email": {
      "type": "email",
      "required": true
    },
    "message": {
      "type": "richtext",
      "required": true,
      "maxLength": 200,
      "minLength": 20
    }
  }
}
